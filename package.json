{
  "name": "svelte-app",
  "version": "3.0.0-rc",
  "scripts": {
    "dev": "run-p routify rollup",
    "dev:nollup": "run-p routify nollup",
    "dev-dynamic": "cross-env BUNDLING=dynamic npm run dev",
    "build": "routify -b && rollup -c && routify export && workbox injectManifest",
    "serve": "spassr --serve-spa --serve-ssr",
    "export": "routify export",
    "build:docker": "npm run build && ./scripts/docker/docker-build.sh",
    "deploy:now": "cd scripts/now && npm run deploy",
    "deploy:netlify": "cd scripts/netlify && npm run deploy",
    "rollup": "rollup -cw",
    "nollup": "nollup -c",
    "routify": "routify"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "@iconify/icons-uil": "^1.0.13",
    "@iconify/svelte": "^1.0.0",
    "@lottiefiles/svelte-lottie-player": "^0.1.4",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "autoprefixer": "^9.8.4",
    "canvas": "^2.6.1",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "del": "^5.1.0",
    "lazysizes": "^5.2.2",
    "nollup": "^0.12.0",
    "npm-run-all": "^4.1.5",
    "postcss-import": "^12.0.1",
    "rollup": "^2.18.1",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-hot": "^0.0.31",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-postcss": "^3.1.2",
    "rollup-plugin-svelte": "^5.2.3",
    "rollup-plugin-svelte-hot": "^0.9.2",
    "rollup-plugin-terser": "^6.1.0",
    "showdown": "^1.9.1",
    "spassr": "^1.1.2",
    "svelte-preprocess": "^4.0.6",
    "svelte-scrollto": "^0.2.0",
    "tailwindcss": "^1.4.6"
  },
  "dependencies": {
    "@rollup/plugin-replace": "^2.3.3",
    "@sveltech/routify": "^1.9.1",
    "svelte": "^3.24.0",
    "workbox-cli": "^5.1.3"
  },
  "routify": {
    "extensions": "svelte,html,svx,md",
    "dynamicImports": true
  }
}
